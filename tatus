[32m+[m[32mfrom turtle import *[m
[32m+[m[32mdef bunka(stred, lr):[m
[32m+[m[32m    penup()[m
[32m+[m[32m    forward(stred)[m
[32m+[m[32m    pendown()[m
[32m+[m[32m    for s in range(6):[m
[32m+[m[32m        forward(60)[m
[32m+[m[32m        if lr=='L':[m
[32m+[m[32m            left(60)[m
[32m+[m[32m        else:[m
[32m+[m[32m            right(60)[m
[32m+[m
[32m+[m[32mbunka(-120,'L')[m
[32m+[m[32mbunka(0,'R')[m
[32m+[m
[32m+[m[32mpenup()[m
[32m+[m[32mforward(60)[m
[32m+[m[32mleft(60)[m
[32m+[m[32mforward(60)[m
[32m+[m[32mright(60)[m
[32m+[m[32mpendown()[m
[32m+[m[32mbunka(0,'L')[m
[32m+[m[32mbunka(0,'R')[m
[32m+[m
[32m+[m[32mpenup()[m
[32m+[m[32mleft(240)[m
[32m+[m[32mforward(60)[m
[32m+[m[32mleft(60)[m
[32m+[m[32mforward(60)[m
[32m+[m[32mright(300)[m
[32m+[m[32mpendown()[m
[32m+[m[32mbunka(0,'R')[m
[32m+[m
[32m+[m[32mpenup()[m
[32m+[m[32mforward(60)[m
[32m+[m[32mright(60)[m
[32m+[m[32mforward(60)[m
[32m+[m[32mleft(60)[m
[32m+[m[32mbunka(0,'L')[m
[32m+[m
[32m+[m[32mpenup()[m
